{
  "name": "agents",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080 --turbopack",
    "dev:start": "pm2 start \"bun run dev\" --log ./logs/dev.log --name dev",
    "dev:logs": "pm2 logs dev",
    "dev:restart": "pm2 restart dev",
    "dev:stop": "pm2 stop dev && pm2 delete dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "NODE_ENV=test vitest run --reporter=verbose",
    "spec": "NODE_ENV=test bun db/scripts/seed-test.ts && NODE_ENV=test playwright test --reporter=list",
    "spec:setup": "NODE_ENV=test bun db/scripts/seed-test.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "bun db/scripts/seed.ts",
    "db:clean": "bun db/scripts/clean.ts",
    "db:squash": "bun db/scripts/squash.ts",
    "db:reset": "bun db/scripts/clean.ts && bun run db:push",
    "shadcn:add": "npx shadcn@latest add"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.57",
    "@base-ui/react": "^1.1.0",
