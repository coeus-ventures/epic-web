    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^4.0.2",
    "baseline-browser-mapping": "^2.9.19",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "server-only": "^0.0.1",
    "shadcn": "^3.5.0",
